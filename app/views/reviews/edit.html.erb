<%= form_for @review do |f| %>


    <div class="mb-3">
    <%= f.label :name %>
    <br>
    <%= f.text_field :name, class: "form-control", placeholder: "Username"%>
     </div>

    
    <div class="mb-3">
     <%= f.submit "Edit Review", class: "btn btn-primary" %>
     </div>
     <%end%>

     