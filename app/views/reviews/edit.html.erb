<%= form_for @review do |f| %>


    <div class="mb-3">
    <%= f.label :title %>
    <br>
    <%= f.text_field :title, class: "form-control", placeholder: "Username"%>
     </div>

     <div class="mb-3">
    <%= f.label :comment %>
    <br>
    <%= f.text_field :comment, class: "form-control", placeholder: "Username"%>
     </div>

    
    <div class="mb-3">
     <%= f.submit "Edit Review", class: "btn btn-primary" %>
     </div>
     <%end%>

     